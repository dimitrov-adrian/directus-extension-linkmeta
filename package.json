{
  "name": "directus-extension-linkmeta",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/dimitrov-adrian/directus-extension-linkmeta.git"
  },
  "author": {
    "email": "dimitrov.adrian+gh@gmail.com",
    "name": "Adrian Dimitrov"
  },
  "directus:extension": {
    "type": "pack",
    "hidden": false,
    "host": "^v9.0.0"
