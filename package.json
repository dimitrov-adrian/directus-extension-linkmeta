    "metascraper-amazon": "^5.25.7",
    "metascraper-audio": "^5.25.7",
    "metascraper-author": "^5.25.7",
    "metascraper-clearbit": "^5.25.7",
    "metascraper-date": "^5.25.7",
    "metascraper-description": "^5.25.7",
    "metascraper-iframe": "^5.25.7",
    "metascraper-image": "^5.25.7",
    "metascraper-lang": "^5.25.7",
    "metascraper-logo": "^5.25.7",
    "metascraper-logo-favicon": "^5.25.7",
    "metascraper-manifest": "^5.25.7",
    "metascraper-publisher": "^5.25.7",
    "metascraper-readability": "^5.25.7",
    "metascraper-soundcloud": "^5.25.7",
